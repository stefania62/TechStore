                                        </Stop>
                                        <Stop offset="1.0">
                                            <color>
                                                <Color red="0.03628167510032654" green="0.0036549707874655724" blue="0.07894736528396606" />
                                            </color>
                                        </Stop>
                                    </stops>
                                </LinearGradient>
                            </textFill>
